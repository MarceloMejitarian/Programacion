#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"


using namespace std;
int main()

{
   int suma,N;
   float i;
   cout<< "Ingrese el valor de N:";
   cin>> N;
   suma=0;
   for (i=1;i<=N;i++)
          suma=suma+pow(i,2);
   cout<< "La suma de los cuadrados de los" << N << "primeros numeros enteros es:" <<suma;
   getch();

   return 0;
}
